<mjml>
    <mj-head>
        <mj-font name="Quantico" href="https://fonts.googleapis.com/css2?family=Quantico" />
        <mj-attributes>
          <mj-text font-family="Quantico, sans-serif" align="center" font-size="18px" line-height="26px" color="#ffffff" padding="0px" />
          <mj-image padding="0px" />
          <mj-button background-color="#D1AA5A" font-size="14px" color="#000000" font-family="Quantico, sans-serif" text-transform="uppercase" border-radius="0px" height="45px" width="200px" />
          <mj-section padding="0px" background-color="#000000" />
          <mj-class name="head-text" font-size="48px" line-height="69px" text-transform="uppercase" />
        </mj-attributes>
    </mj-head>
    <mj-body background="#F1F1F1">
    <!-- logo section -->
    <mj-section>
        <mj-column>
        <mj-image padding="20px" width="125px" src="data:image/png;base64," alt="logo"/>
        </mj-column>
        
    </mj-section>
    <!-- navbar -->
    <mj-section background-color="#ef6451">
      <mj-column>
        <mj-navbar base-url="https://mjml.io" hamburger="hamburger" ico-color="#ffffff">
            <mj-navbar-link href="/gettings-started-onboard" color="#ffffff">Getting started</mj-navbar-link>
            <mj-navbar-link href="/try-it-live" color="#ffffff">Try it live</mj-navbar-link>
            <mj-navbar-link href="/templates" color="#ffffff">Templates</mj-navbar-link>
            <mj-navbar-link href="/components" color="#ffffff">Components</mj-navbar-link>
        </mj-navbar>
      </mj-column>
    </mj-section>
    <!-- hero section -->
    <mj-section>
    <mj-column>
        <mj-image src="https://brand.assets.adidas.com/image/upload/f_auto,q_auto,fl_lossy/enDE/Images/400X248_01_tcm346-956545.jpg" alt="hero" />

       </mj-column> 
    </mj-section>
    <!-- carousel -->
    <mj-section>
      <mj-column>
        <mj-carousel>
          <mj-carousel-image src="https://assets.adidas.com/images/h_840,f_auto,q_auto,fl_lossy,c_fill,g_auto/61f87dec481e4512823ea7fb0080ba1a_9366/Gazelle_Shoes_Black_BB5476_01_standard.jpg" />
          <mj-carousel-image src="https://assets.adidas.com/images/h_840,f_auto,q_auto,fl_lossy,c_fill,g_auto/61f87dec481e4512823ea7fb0080ba1a_9366/Gazelle_Shoes_Black_BB5476_01_standard.jpg" />
          <mj-carousel-image src="https://assets.adidas.com/images/h_840,f_auto,q_auto,fl_lossy,c_fill,g_auto/61f87dec481e4512823ea7fb0080ba1a_9366/Gazelle_Shoes_Black_BB5476_01_standard.jpg" />
        </mj-carousel>
      </mj-column>
    </mj-section>
    <!-- discount section -->
    <mj-section>
    <mj-column>
        <mj-image src="https://assets.adidas.com/images/h_840,f_auto,q_auto,fl_lossy,c_fill,g_auto/61f87dec481e4512823ea7fb0080ba1a_9366/Gazelle_Shoes_Black_BB5476_01_standard.jpg" alt="discount-image"/>
    <mj-button >Get discount<mj-button>
        
    </mj-button>
     </mj-column> 
    </mj-section>
    
    <!-- background image section -->
    <mj-section background-url="https://1.bp.blogspot.com/-TPrfhxbYpDY/Uh3Refzk02I/AAAAAAAALw8/5sUJ0UUGYuw/s1600/New+York+in+The+1960's+-+70's+(2).jpg"
              background-size="cover"
              background-repeat="no-repeat"
              
              >

    <mj-column>
    <mj-text  align="center">
      Slogan here
    </mj-text>

      <mj-button href="#">
        Promotion
      </mj-button>

    </mj-column>

  </mj-section>
    
    </mj-body>
</mjml>